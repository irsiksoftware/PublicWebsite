{
  "name": "testforai",
  "version": "1.0.0",
  "type": "module",
  "description": "AI Agent Swarm Dashboard",
  "scripts": {
    "build": "npm run images:optimize && webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "webpack serve --mode development",
    "watch": "webpack --mode development --watch",
    "lint": "eslint js/**/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "cypress open",
    "e2e:headless": "cypress run",
    "css:optimize": "postcss css/**/*.css --dir public/css --no-map",
    "build:css": "npm run css:optimize",
    "images:optimize": "node ./build-scripts/optimize-images.js",
    "accessibility:audit": "node accessibility-audit.js",
    "a11y:audit": "node accessibility-audit.js",
    "a11y:test": "node accessibility-audit.js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@lhci/cli": "^0.15.1",
    "@types/jest": "^30.0.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.11.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.1.1",
    "cypress": "^15.4.0",
    "eslint": "^8.57.0",
    "glob": "^11.0.3",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "mini-css-extract-plugin": "^2.9.4",
    "pa11y": "^9.0.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "responsive-loader": "^3.1.2",
    "sharp": "^0.34.4",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "private": true
}
