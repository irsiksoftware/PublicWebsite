{
  "name": "testforai",
  "version": "1.0.0",
  "type": "module",
  "description": "AI Agent Swarm Dashboard",
  "scripts": {
    "build": "npm run images:optimize && webpack --config config/webpack.config.cjs --mode production",
    "build:dev": "webpack --config config/webpack.config.cjs --mode development",
    "start": "webpack serve --config config/webpack.config.cjs --mode development",
    "watch": "webpack --config config/webpack.config.cjs --mode development --watch",
    "lint": "eslint --config config/eslint.config.cjs js/**/*.js",
    "test": "jest --config config/jest.config.cjs",
    "test:watch": "jest --config config/jest.config.cjs --watch",
    "test:coverage": "jest --config config/jest.config.cjs --coverage",
    "cypress": "cypress open --config-file config/cypress.config.js",
    "cypress:headless": "cypress run --config-file config/cypress.config.js",
    "e2e": "cypress open --config-file config/cypress.config.js",
    "e2e:headless": "cypress run --config-file config/cypress.config.js",
    "css:optimize": "postcss --config config/postcss.config.js css/**/*.css --dir public/css --no-map",
    "build:css": "npm run css:optimize",
    "images:optimize": "node ./build-scripts/optimize-images.js",
    "accessibility:audit": "node js/accessibility-audit.js",
    "a11y:audit": "node js/accessibility-audit.js",
    "a11y:test": "node js/accessibility-audit.js",
    "sandbox": "node sandbox/server.js",
    "sandbox:start": "node sandbox/server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@lhci/cli": "^0.15.1",
    "@types/jest": "^30.0.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.11.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.1.1",
    "cypress": "^15.4.0",
    "eslint": "^8.57.0",
    "glob": "^11.0.3",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "mini-css-extract-plugin": "^2.9.4",
    "pa11y": "^9.0.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "responsive-loader": "^3.1.2",
    "sharp": "^0.34.4",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "private": true
}
